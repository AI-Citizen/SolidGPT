    "test": "father test",
    "now-build": "npm run build"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.0.1",
    "@types/classnames": "^2.2.10",
    "@types/enzyme": "^3.10.7",
    "@types/jest": "^26.0.4",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "cross-env": "^7.0.0",
    "dumi": "^2.1.0",
    "enzyme": "^3.3.0",
    "enzyme-to-json": "^3.4.0",
    "eslint": "^7.8.1",
    "father": "^2.13.2",
    "gh-pages": "^3.1.0",
    "less": "^3.10.3",
    "np": "^6.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@babel/runtime": "^7.10.1",
    "classnames": "2.x",
    "rc-motion": "^2.6.0",
    "rc-util": "^5.20.1"
  }
